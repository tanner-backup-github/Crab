(define add-400 (lambda (x) (+ x 400)))

(print (add-400 20))
(define z (add-400 10))
(define x 40)

(define Ϯdo-things (lambda (x) (if (> z x) (+ x 1) (+ x 0))))

(print (Ϯdo-things 40))

(define ☭ 9001)
(print ☭)

(define π 3.14)
(print π)
	
(print (> ☭ π))

(define τ (* π 2))
(define pi π)
(define tau τ)

(print (* π τ))
(print (* pi tau))


(if (> 40 30)
	(print 40)
	(print 30))